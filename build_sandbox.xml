<!-- compute means generating as described, than compile the apps we depend on -->
<project>
	<property name="app_properties" value="apps/sandbox/org/molgenis/sandbox/sandbox.properties" />
	<property name="app_src" value="apps/sandbox:apps/pheno:handwritten/java" />
		
	<target name="clean">
		<ant antfile="build_common.xml" target="clean" inheritAll="true">
		</ant>
	</target>
	
	<target name="generate">
		<ant antfile="build_common.xml" target="generate" inheritAll="true"/>
		<ant target="compile" inheritAll="true"/>
	</target>
	
	<target name="compile">
		<ant antfile="build_common.xml" target="compile" inheritAll="true"/>
	</target>
	
	<target name="clean_generate" depends="clean,generate,compile"/>

</project>
