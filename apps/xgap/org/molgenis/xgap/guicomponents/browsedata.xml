<molgenis>
	<form label="Browse data" header="Navigate investigations (studies)" name="Investigations" entity="Investigation"
		compact_view="name" group="biologist">
		<menu name="InvestigationMenu">
			<plugin name="Overview" label="Investigation overview"
				type="plugins.investigationoverview.InvestigationOverviewPlugin"
				group="biologist" />
			<form name="Datas" label="Data matrices" header="Navigate data matrices" entity="Data" compact_view="name"
				group="biologist">
				<menu name="Select" group="biologist">
					<plugin label="View" name="Manager" type="plugins.matrix.manager.MatrixManager"
						group="biologist" />
					<plugin label="Inspect" name="Inspector" type="plugins.matrix.inspector.MatrixInspector"
						group="biologist" />
					<plugin name="MatrixAdmin" label="Delete"
						type="plugins.matrix.admin.MatrixAdmin" group="biologist" />
					<!--plugin name="MatrixTests" type="org.molgenis.sandbox.ui.MatrixTests"
						label="Viewer v2 (IN DEV)" group="biologist" /-->
				</menu>
			</form>
			<menu name="BasicAnnotations" label="Annotations" header="Navigate annotations"> <!-- header does not work yet for menu -->
				<form name="Individuals" entity="Individual" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
				<form name="Panels" entity="Panel" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
				<form name="Chromosomes" entity="Chromosome" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
				<form name="Markers" entity="Marker" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
				<form name="Genes" entity="Gene" group="biologist" hide_fields="deleted,deletionTime,deletedBy">
					<form name="Transcripts" entity="Transcript" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
				</form>
				<form name="Measurements" entity="Measurement" hide_fields="targettypeAllowedForRelation,panelLabelAllowedForRelation,deleted,deletionTime,deletedBy" group="biologist" />
				<menu name="AdvancedAnnotations" label="More annotations" group="biologist">
					<form name="DerivedTraits" entity="DerivedTrait" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
					<form name="EnvironmentalFactors" entity="EnvironmentalFactor" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
					<form name="MassPeaks" entity="MassPeak" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
					<form name="Metabolites" entity="Metabolite" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
					<form name="Probes" entity="Probe" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
					<form name="ProbeSets" entity="ProbeSet" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
					<form name="SNPs" entity="SNP" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
					<form name="Polymorphisms" entity="Polymorphism" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
					<form name="Samples" entity="Sample" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
					<form name="Spots" entity="Spot" group="biologist" hide_fields="deleted,deletionTime,deletedBy" />
				</menu>
			</menu>
		</menu>
	</form>
</molgenis>