<molgenis>

	<plugin label="Login" name="UserLogin" type="org.molgenis.auth.ui.UserLogin" />
	
	<form label="Browse data" name="Investigations" entity="Investigation" compact_view="name" group="biologist">
			<menu name="InvestigationMenu">
				<plugin name="Overview" type="plugins.investigationoverview.InvestigationOverviewPlugin" group="biologist"/>
				<form name="Datas" label="View data" entity="Data" compact_view="name" group="biologist">
					<menu name="Select" group="biologist">
						<plugin name="Manager" type="plugins.matrix.manager.MatrixManager" group="biologist" />
						<plugin name="Heatmap" type="plugins.matrix.heatmap.MatrixHeatmap" group="biologist" />
						<plugin name="Rplot" type="plugins.rplot.RplotPlugin" group="biologist" />
						<plugin name="Inspector" type="plugins.matrix.inspector.MatrixInspector" group="biologist" />
						<plugin name="MatrixAdmin" label="Delete storage" type="plugins.matrix.admin.MatrixAdmin" group="biologist" />
						<!--plugin name="ManagerDev" type="plugins.matrix_dev.manager.MatrixManager" group="biologist" /-->
					</menu>
				</form>
				<menu name="BasicAnnotations" label="View annotations">
					<form name="Individuals" entity="Individual" group="biologist" />
					<form name="Panels" entity="Panel" group="biologist" />
					<form name="Chromosomes" entity="Chromosome" group="biologist" />
					<form name="Markers" entity="Marker" group="biologist" />
					<form name="Genes" entity="Gene" group="biologist" />
					<form name="Measurements" entity="Measurement" hide_fields="targettypeAllowedForRelation,panelLabelAllowedForRelation" group="biologist" />
					<menu name="AdvancedAnnotations" label="More annotations" group="biologist">
						<form name="DerivedTraits" entity="DerivedTrait" group="biologist" />
						<form name="EnvironmentalFactors" entity="EnvironmentalFactor" group="biologist" />
						<form name="MassPeaks" entity="MassPeak" group="biologist" />
						<form name="Metabolites" entity="Metabolite" group="biologist" />
						<form name="Probes" entity="Probe" group="biologist" />
						<form name="Samples" entity="Sample" group="biologist" />
						<form name="Spots" entity="Spot" group="biologist" />
					</menu>
				</menu>
				<form name="Files" entity="InvestigationFile" label="View files" group="biologist">
					<plugin name="Viewer" type="plugins.molgenisfile.MolgenisFileManager" group="biologist" />
				</form>
			</menu>
		</form>
		
		<menu name="ImportDataMenu" label="Import data" group="biologist">
			<plugin label="R/qtl" name="QTLWizard" type="plugins.xgapwizard.QTLDataSetWizard" group="biologist" />
			<plugin label="Plink" name="PlinkFileManager" type="plugins.molgenisfile.plink.PlinkFileManager" group="biologist" />
		</menu>

		<plugin label="Run QTL mapping" name="Cluster" type="plugins.cluster.ClusterPlugin" group="biologist" />

		<menu name="AnalysisSettings" label="Configure analysis" group="bioinformatician">
			<form name="Analysis" label="Analyses" entity="Analysis" group="bioinformatician" />
			<form name="ParameterSets" label="Parameters" entity="ParameterSet" group="bioinformatician">
				<form name="ParameterNames" entity="ParameterName" group="bioinformatician">
					<form name="ParameterValues" entity="ParameterValue" group="bioinformatician" />
				</form>
			</form>
			<form name="DataSets" label="Datasets" entity="DataSet" group="bioinformatician">
				<form name="DataNames" entity="DataName" group="bioinformatician">
					<form name="DataValues" entity="DataValue" group="bioinformatician" />
				</form>
			</form>
			<form name="RScripts" label="R scripts" entity="RScript" group="bioinformatician">
				<plugin name="RViewer" type="plugins.molgenisfile.MolgenisFileManager" group="bioinformatician"/>
				<!--plugin label="Help" name="AddNewToolsHelp" type="plugins.cluster.addnewtoolshelp" group="bioinformatician" /-->
			</form>
			<plugin label="Tag data" name="MatrixWizard" type="plugins.xgapwizard.MatrixWizard" group="bioinformatician" />
		</menu>

		<plugin label="Search" name="SimpleDbSearch" type="plugin.search.SimpleWholeDatabaseSearch" group="biologist" />

		<menu label="Settings" name="Settings">
			<menu name="Auth" label="Users and permissions">
				<menu label="Basic management" name="Management">
					<form name="MolgenisUser" entity="MolgenisUser" />
					<form name="MolgenisGroup" entity="MolgenisGroup"/>
					<form name="MolgenisRoleGroupLink" entity="MolgenisRoleGroupLink" />
					<form name="MolgenisEntity" entity="MolgenisEntity" view="list" label="Entities" />
					<form name="MolgenisPermission" entity="MolgenisPermission" />
				</menu>
				<plugin label="My permissions" name="PermissionManagementPlugin" type="org.molgenis.auth.service.permissionmanagement.PermissionManagementPlugin" />
			</menu>
			<plugin label="Database status" name="DatabaseSettings" type="plugins.system.database.Settings" />
			<plugin label="File storage" name="FileStorage" type="plugins.system.settings.Settings" />
			<plugin label="Install R packages" name="DependencyManager" type="plugins.cluster.demo.dependencymanager.DependencyManager" />
			<menu name="Ontologies" label="Ontologies" group="biologist">
				<form name="Species" entity="Species" group="biologist" />
				<form name="OntologyTerms" entity="OntologyTerm" group="biologist" />
			</menu>
			<menu name="OtherAdmin" label="Utilities">
				<plugin label="Advanced import" name="ExcelWizard" type="plugins.genericwizard.GenericWizard" group="biologist" />
				<plugin label="Format names" name="NamingPolicy" type="plugins.help.naming.NamingPlugin" group="biologist" />
				<plugin label="Rename duplicates" name="DuplicateRenaming" type="plugins.help.duplicates.DuplicatesPlugin" group="biologist" />
				<plugin label="KEGG converter" name="KeggConverter" type="plugins.keggplugin.KeggToolsPlugin" group="bioinformatician" />
				<form label="Job table" name="Jobs" entity="Job">
					<form name="Subjobs" entity="Subjob" />
				</form>
				<!--plugin name="DatabaseArchiver" type="plugins.archiveexportimport.ArchiveExportImportPlugin" /-->
			</menu>
		</menu>
		
</molgenis>