            <class>org.molgenis.protocol.WorkflowElementParameter</class>
            <class>org.molgenis.auth.MolgenisRole</class>
            <class>org.molgenis.auth.MolgenisGroup</class>
            <class>org.molgenis.auth.MolgenisRoleGroupLink</class>
            <class>org.molgenis.auth.Person</class>
