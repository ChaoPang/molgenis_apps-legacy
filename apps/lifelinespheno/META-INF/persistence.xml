            <class>org.molgenis.pheno.Species</class>
            <class>org.molgenis.pheno.ObservationElement</class>
            <class>org.molgenis.pheno.ObservationTarget</class>
            <class>org.molgenis.pheno.ObservableFeature</class>
            <class>org.molgenis.pheno.Measurement</class>
