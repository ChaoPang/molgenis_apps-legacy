<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" 
             xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
    <persistence-unit name="molgenis" transaction-type="RESOURCE_LOCAL">
        <class>org.molgenis.core.OntologyTerm</class>
            <class>org.molgenis.core.Ontology</class>
            <class>org.molgenis.core.MolgenisFile</class>
            <class>org.molgenis.core.RuntimeProperty</class>
            <class>org.molgenis.core.Publication</class>
            <class>org.molgenis.core.UseCase</class>
            <class>org.molgenis.core.MolgenisEntity</class>
