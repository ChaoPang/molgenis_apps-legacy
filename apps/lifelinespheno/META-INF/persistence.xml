            <class>org.molgenis.protocol.ProtocolDocument</class>
            <class>org.molgenis.protocol.Workflow</class>
            <class>org.molgenis.protocol.WorkflowElement</class>
            <class>org.molgenis.protocol.WorkflowElementParameter</class>
            <class>org.molgenis.auth.MolgenisRole</class>
            <class>org.molgenis.auth.MolgenisGroup</class>
            <class>org.molgenis.auth.MolgenisRoleGroupLink</class>
            <class>org.molgenis.auth.Person</class>
            <class>org.molgenis.auth.Institute</class>
            <class>org.molgenis.auth.MolgenisUser</class>
            <class>org.molgenis.auth.MolgenisPermission</class>
            <class>org.molgenis.organization.Investigation_Contacts</class>
            <class>org.molgenis.pheno.Panel_Individuals</class>
            <class>org.molgenis.pheno.Panel_FounderPanels</class>
            <class>org.molgenis.pheno.Code_Feature</class>
            <class>org.molgenis.protocol.Protocol_Features</class>
            <class>org.molgenis.protocol.ProtocolApplication_Performer</class>
            <class>org.molgenis.protocol.WorkflowElement_PreviousSteps</class>
<!--         <class>org.molgenis.lifelines.Entities.PublicDictionary</class> -->
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/LL_IMPORT_TEST"/>
            <property name="javax.persistence.jdbc.password" value="molgenis"/>
            <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
            <property name="javax.persistence.jdbc.user" value="molgenis"/>
            <property name="javax.persistence.validation.mode" value="none"/>    
            <property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect"/>
            <property name="hibernate.show_sql" value="true"/>
          
            <property name="hibernate.c3p0.max_size" value="13"/>
            <property name="hibernate.c3p0.min_size" value="13"/>
            <property name="hibernate.c3p0.timeout" value="5000"/>
            <property name="hibernate.c3p0.max_statements" value="100"/>
            <property name="hibernate.c3p0.idle_test_period" value="300"/>
            <property name="hibernate.c3p0.acquire_increment" value="2"/>
          
          <!--
          Automatically validates or exports schema DDL to the database when the SessionFactory is created. 
          With create-drop, the database schema will be dropped when the SessionFactory is closed explicitly.
		e.g. validate | update | create | create-drop
           -->
            <property name="hibernate.hbm2ddl.auto" value="create-drop"/>
           
        </properties>
    </persistence-unit>
