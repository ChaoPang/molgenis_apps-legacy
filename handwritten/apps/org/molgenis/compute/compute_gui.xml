		<form name="ComputeProtocols" entity="ComputeProtocol"
			hide_fields="Investigation,ontologyReference,TargetFilter,Contact,ProtocolType"
			viewtype="record">
			<form name="ComputeFeatures" entity="ComputeFeature"
				hide_fields="Investigation,ontologyReference,temporal"
				viewtype="list" />
		</form>
		<form name="Workflows" entity="Workflow" viewtype="record"
