			</menu>
			<plugin name="GenericWizard" type="plugins.genericwizard.GenericWizard"
				label="Excel Import" />
			<plugin name="GenericExport"
				type="plugins.archiveexportimport.ArchiveExportImportPlugin" label="Excel export" />
	</menu>
