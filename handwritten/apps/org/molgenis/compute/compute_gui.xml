					hide_fields="ontologyReference,relation" />
			</form>
			<menu name="Console">
				<plugin name="console" type="org.molgenis.compute.ConsolePlugin" />
			</menu>
			<menu name="Admin">
				<form name="AllComputeProtocols" entity="ComputeProtocol"
					viewtype="list" />
				<form name="AllComputeFeatures" entity="ComputeFeature"
